zif1='abcdefb'
zif2='jhzefa'

def fa(str1,str2):
    lenght=len(str1)
    for i in range(lenght,0,-1):
        print(i)
        for j in range(0,lenght-i+1):#7-6+1 2
            print(j)
            tp=str1[j:i+j]#0 6+2
            print(tp,j,i+j)
            if str2.find(tp)>-1:
                print(str2.find(tp))
                return tp

print(fa(zif1,zif2))
